{"ast":null,"code":"import \"core-js/modules/es.array.concat.js\";\nimport \"core-js/modules/es.array.filter.js\";\nimport \"core-js/modules/es.array.join.js\";\nimport \"core-js/modules/es.array.map.js\";\nimport \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.array.splice.js\";\nimport \"core-js/modules/es.function.name.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport DangerousHTML from 'dangerous-html/vue';\nimport AppNavigation from '../components/navigation';\nimport AppFooter from '../components/footer';\nexport default {\n  name: 'TaskTemplateBuilder',\n  props: {},\n  components: {\n    AppNavigation: AppNavigation,\n    DangerousHTML: DangerousHTML,\n    AppFooter: AppFooter\n  },\n  data: function data() {\n    return {\n      currentStep: 1,\n      taskData: {\n        title: '',\n        description: '',\n        difficulty: 'medium',\n        functionName: '',\n        parameters: [{\n          name: '',\n          type: 'int'\n        }],\n        returnType: 'int',\n        functionDescription: '',\n        language: 'python',\n        libraries: [],\n        inputFormat: '',\n        outputFormat: '',\n        tests: [{\n          input: '',\n          expectedOutput: '',\n          isPublic: true\n        }]\n      },\n      availableLibraries: [{\n        id: 'numpy',\n        name: 'NumPy'\n      }, {\n        id: 'pandas',\n        name: 'Pandas'\n      }, {\n        id: 'matplotlib',\n        name: 'Matplotlib'\n      }, {\n        id: 'requests',\n        name: 'Requests'\n      }]\n    };\n  },\n  methods: {\n    nextStep: function nextStep() {\n      if (this.currentStep < 5) {\n        this.currentStep++;\n      }\n    },\n    previousStep: function previousStep() {\n      if (this.currentStep > 1) {\n        this.currentStep--;\n      }\n    },\n    addParameter: function addParameter() {\n      this.taskData.parameters.push({\n        name: '',\n        type: 'int'\n      });\n    },\n    removeParameter: function removeParameter(index) {\n      if (this.taskData.parameters.length > 1) {\n        this.taskData.parameters.splice(index, 1);\n      }\n    },\n    toggleLibrary: function toggleLibrary(libId) {\n      var index = this.taskData.libraries.indexOf(libId);\n      if (index > -1) {\n        this.taskData.libraries.splice(index, 1);\n      } else {\n        this.taskData.libraries.push(libId);\n      }\n    },\n    addTest: function addTest() {\n      this.taskData.tests.push({\n        input: '',\n        expectedOutput: '',\n        isPublic: true\n      });\n    },\n    removeTest: function removeTest(index) {\n      if (this.taskData.tests.length > 1) {\n        this.taskData.tests.splice(index, 1);\n      }\n    },\n    generateFunctionTemplate: function generateFunctionTemplate() {\n      if (!this.taskData.functionName) return '// Введите имя функции';\n      var params = this.taskData.parameters.filter(function (p) {\n        return p.name;\n      }).map(function (p) {\n        return \"\".concat(p.name, \": \").concat(p.type);\n      }).join(', ');\n      var template = \"def \".concat(this.taskData.functionName, \"(\").concat(params, \")\");\n      if (this.taskData.returnType !== 'void') {\n        template += \" -> \".concat(this.taskData.returnType);\n      }\n      template += \":\\n    \\\"\\\"\\\"\\n    \".concat(this.taskData.functionDescription || 'Описание функции', \"\\n    \\\"\\\"\\\"\\n    # \\u0412\\u0430\\u0448\\u0430 \\u0440\\u0435\\u0430\\u043B\\u0438\\u0437\\u0430\\u0446\\u0438\\u044F \\u0437\\u0434\\u0435\\u0441\\u044C\\n    pass\");\n      return template;\n    },\n    generateMainCallExample: function generateMainCallExample() {\n      if (!this.taskData.functionName) return '// Введите имя функции';\n      var params = this.taskData.parameters.filter(function (p) {\n        return p.name;\n      }).map(function (p) {\n        return p.name;\n      }).join(', ');\n      var example = \"if __name__ == \\\"__main__\\\":\\n    # \\u041F\\u0440\\u0438\\u043C\\u0435\\u0440 \\u0432\\u044B\\u0437\\u043E\\u0432\\u0430 \\u0444\\u0443\\u043D\\u043A\\u0446\\u0438\\u0438\\n    result = \".concat(this.taskData.functionName, \"(\").concat(params, \")\\n    \");\n      if (this.taskData.returnType !== 'void') {\n        example += \"print(f\\\"\\u0420\\u0435\\u0437\\u0443\\u043B\\u044C\\u0442\\u0430\\u0442: {result}\\\")\";\n      }\n      return example;\n    },\n    saveTask: function saveTask() {\n      // Здесь будет логика сохранения задачи\n      console.log('Сохранение задачи:', this.taskData);\n      alert('Задача успешно сохранена!');\n    }\n  },\n  metaInfo: {\n    title: 'Конструктор задач - CodeCraft',\n    meta: [{\n      property: 'og:title',\n      content: 'Конструктор задач - CodeCraft'\n    }]\n  }\n};","map":{"version":3,"names":["DangerousHTML","AppNavigation","AppFooter","name","props","components","data","currentStep","taskData","title","description","difficulty","functionName","parameters","type","returnType","functionDescription","language","libraries","inputFormat","outputFormat","tests","input","expectedOutput","isPublic","availableLibraries","id","methods","nextStep","previousStep","addParameter","push","removeParameter","index","length","splice","toggleLibrary","libId","indexOf","addTest","removeTest","generateFunctionTemplate","params","filter","p","map","concat","join","template","generateMainCallExample","example","saveTask","console","log","alert","metaInfo","meta","property","content"],"sources":["src/views/task-template-builder.vue"],"sourcesContent":["<template>\n  <div class=\"task-template-builder-container10\">\n    <app-navigation></app-navigation>\n\n    <div class=\"task-template-builder-container11\">\n      <div class=\"task-template-builder-container12\">\n        <DangerousHTML\n            html=\"<style>\n  #hero-section {\n    min-height: 100vh;\n    display: flex;\n    align-items: center;\n    padding: var(--spacing-4xl) var(--spacing-lg);\n    background: var(--color-surface);\n    position: relative;\n  }\n  #hero-section::before {\n    content: '';\n    position: absolute;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    background-image: repeating-linear-gradient(\n        0deg,\n        transparent,\n        transparent 2px,\n        color-mix(in srgb, var(--color-border) 10%, transparent) 2px,\n        color-mix(in srgb, var(--color-border) 10%, transparent) 4px\n      );\n    pointer-events: none;\n    z-index: 1;\n    opacity: 0.3;\n  }\n  #process-section {\n    padding: var(--spacing-3xl) var(--spacing-2xl);\n    background: var(--color-surface);\n  }\n  #settings-section {\n    padding: var(--spacing-4xl) var(--spacing-lg);\n    background: var(--color-surface);\n  }\n  #docs-section {\n    padding: var(--spacing-4xl) var(--spacing-lg);\n    background: var(--color-surface);\n  }\n  #documentation-section {\n    padding: var(--spacing-4xl) var(--spacing-lg);\n    background: var(--color-surface);\n  }\n  #dashboard-section {\n    padding: var(--spacing-4xl) var(--spacing-lg);\n    background: var(--color-surface);\n  }\n  #stats-section {\n    padding: var(--spacing-4xl) var(--spacing-lg);\n    background: var(--color-surface);\n  }\n  #faq-section {\n    padding: var(--spacing-4xl) var(--spacing-lg);\n    background: var(--color-surface);\n  }\n  @media (prefers-reduced-motion: reduce) {\n  *, *::before, *::after {\n    animation-duration: 0.01ms !important;\n    animation-iteration-count: 1 !important;\n    transition-duration: 0.01ms !important;\n  }\n  }\n  </style>\"\n        ></DangerousHTML>\n      </div>\n    </div>\n\n    <section\n        id=\"hero-section\"\n        role=\"region\"\n        aria-label=\"Конструктор задач\"\n    >\n      <div class=\"hero-container\">\n        <div class=\"hero-panel\">\n          <div class=\"hero-grid\">\n            <div class=\"hero-head\">\n              <h1 class=\"hero-title task-template-builder-hero-title\">\n                Конструктор задач\n              </h1>\n              <p class=\"hero-subtitle\">Пошаговое создание учебных заданий</p>\n            </div>\n\n            <!-- Шаги конструктора -->\n            <div class=\"wizard-container\">\n              <!-- Индикатор шагов -->\n              <nav role=\"navigation\" aria-label=\"Шаги создания задачи\" class=\"stepper\">\n                <div :class=\"{'step-item': true, 'active': currentStep === 1}\">\n                  <div aria-hidden=\"true\" class=\"step-number\"><span>1</span></div>\n                  <div class=\"step-content\">\n                    <h3 class=\"step-title\">Основная информация</h3>\n                    <p class=\"step-hint\">Название и описание</p>\n                  </div>\n                </div>\n                <div :class=\"{'step-item': true, 'active': currentStep === 2}\">\n                  <div aria-hidden=\"true\" class=\"step-number\"><span>2</span></div>\n                  <div class=\"step-content\">\n                    <h3 class=\"step-title\">Сигнатура функции</h3>\n                    <p class=\"step-hint\">Определение функции</p>\n                  </div>\n                </div>\n                <div :class=\"{'step-item': true, 'active': currentStep === 3}\">\n                  <div aria-hidden=\"true\" class=\"step-number\"><span>3</span></div>\n                  <div class=\"step-content\">\n                    <h3 class=\"step-title\">Конфигурация</h3>\n                    <p class=\"step-hint\">Язык и библиотеки</p>\n                  </div>\n                </div>\n                <div :class=\"{'step-item': true, 'active': currentStep === 4}\">\n                  <div aria-hidden=\"true\" class=\"step-number\"><span>4</span></div>\n                  <div class=\"step-content\">\n                    <h3 class=\"step-title\">Вызов из main</h3>\n                    <p class=\"step-hint\">Точка входа</p>\n                  </div>\n                </div>\n                <div :class=\"{'step-item': true, 'active': currentStep === 5}\">\n                  <div aria-hidden=\"true\" class=\"step-number\"><span>5</span></div>\n                  <div class=\"step-content\">\n                    <h3 class=\"step-title\">Тесты</h3>\n                    <p class=\"step-hint\">Проверка решения</p>\n                  </div>\n                </div>\n              </nav>\n\n              <!-- Контент шагов -->\n              <div class=\"wizard-content\">\n                <!-- Шаг 1: Основная информация -->\n                <div v-if=\"currentStep === 1\" class=\"step-panel\">\n                  <h2 class=\"step-title\">Основная информация о задаче</h2>\n                  <div class=\"form-group\">\n                    <label for=\"task-title\">Название задачи</label>\n                    <input\n                        type=\"text\"\n                        id=\"task-title\"\n                        v-model=\"taskData.title\"\n                        placeholder=\"Введите название задачи\"\n                    >\n                  </div>\n                  <div class=\"form-group\">\n                    <label for=\"task-description\">Описание задачи</label>\n                    <textarea\n                        id=\"task-description\"\n                        v-model=\"taskData.description\"\n                        rows=\"5\"\n                        placeholder=\"Опишите условие задачи, что должен сделать студент\"\n                    ></textarea>\n                  </div>\n                  <div class=\"form-group\">\n                    <label for=\"task-difficulty\">Уровень сложности</label>\n                    <select id=\"task-difficulty\" v-model=\"taskData.difficulty\">\n                      <option value=\"easy\">Легкий</option>\n                      <option value=\"medium\">Средний</option>\n                      <option value=\"hard\">Сложный</option>\n                    </select>\n                  </div>\n                </div>\n\n                <!-- Шаг 2: Сигнатура функции -->\n                <div v-if=\"currentStep === 2\" class=\"step-panel\">\n                  <h2 class=\"step-title\">Определение сигнатуры функции</h2>\n                  <div class=\"form-group\">\n                    <label for=\"function-name\">Имя функции</label>\n                    <input\n                        type=\"text\"\n                        id=\"function-name\"\n                        v-model=\"taskData.functionName\"\n                        placeholder=\"Введите имя функции\"\n                    >\n                  </div>\n                  <div class=\"form-group\">\n                    <label for=\"function-params\">Параметры функции</label>\n                    <div class=\"params-container\">\n                      <div v-for=\"(param, index) in taskData.parameters\" :key=\"index\" class=\"param-row\">\n                        <input\n                            type=\"text\"\n                            v-model=\"param.name\"\n                            placeholder=\"Имя параметра\"\n                        >\n                        <select v-model=\"param.type\">\n                          <option value=\"int\">int</option>\n                          <option value=\"float\">float</option>\n                          <option value=\"str\">str</option>\n                          <option value=\"list\">list</option>\n                          <option value=\"dict\">dict</option>\n                          <option value=\"bool\">bool</option>\n                        </select>\n                        <button @click=\"removeParameter(index)\" class=\"btn-remove\">×</button>\n                      </div>\n                      <button @click=\"addParameter\" class=\"btn-outline btn\">+ Добавить параметр</button>\n                    </div>\n                  </div>\n                  <div class=\"form-group\">\n                    <label for=\"return-type\">Тип возвращаемого значения</label>\n                    <select id=\"return-type\" v-model=\"taskData.returnType\">\n                      <option value=\"int\">int</option>\n                      <option value=\"float\">float</option>\n                      <option value=\"str\">str</option>\n                      <option value=\"list\">list</option>\n                      <option value=\"dict\">dict</option>\n                      <option value=\"bool\">bool</option>\n                      <option value=\"void\">void (нет возврата)</option>\n                    </select>\n                  </div>\n                  <div class=\"form-group\">\n                    <label for=\"function-description\">Описание функции</label>\n                    <textarea\n                        id=\"function-description\"\n                        v-model=\"taskData.functionDescription\"\n                        rows=\"3\"\n                        placeholder=\"Опишите, что должна делать функция\"\n                    ></textarea>\n                  </div>\n                </div>\n\n                <!-- Шаг 3: Конфигурация -->\n                <div v-if=\"currentStep === 3\" class=\"step-panel\">\n                  <h2 class=\"step-title\">Конфигурация шаблона</h2>\n                  <div class=\"form-group\">\n                    <label for=\"language-select\">Язык программирования</label>\n                    <select id=\"language-select\" v-model=\"taskData.language\">\n                      <option value=\"python\">Python 3.10</option>\n                      <option value=\"cpp\">C++ 17</option>\n                      <option value=\"java\">Java 11</option>\n                      <option value=\"csharp\">C# 10</option>\n                    </select>\n                  </div>\n                  <div class=\"library-section\">\n                    <label>Библиотеки</label>\n                    <div class=\"chip-container\">\n                      <button\n                          v-for=\"lib in availableLibraries\"\n                          :key=\"lib.id\"\n                          @click=\"toggleLibrary(lib.id)\"\n                          :class=\"{'chip': true, 'active': taskData.libraries.includes(lib.id)}\"\n                          role=\"button\"\n                          :aria-pressed=\"taskData.libraries.includes(lib.id)\"\n                      >\n                        <svg\n                            width=\"16\"\n                            xmlns=\"http://www.w3.org/2000/svg\"\n                            height=\"16\"\n                            viewBox=\"0 0 24 24\"\n                        >\n                          <path\n                              d=\"m16 6l4 14M12 6v14M8 8v12M4 4v16\"\n                              fill=\"none\"\n                              stroke=\"currentColor\"\n                              stroke-width=\"2\"\n                              stroke-linecap=\"round\"\n                              stroke-linejoin=\"round\"\n                          ></path>\n                        </svg>\n                        <span>{{ lib.name }}</span>\n                      </button>\n                    </div>\n                  </div>\n                  <div class=\"template-preview\">\n                    <label>Шаблон функции</label>\n                    <pre><span>{{ generateFunctionTemplate() }}</span></pre>\n                  </div>\n                </div>\n\n                <!-- Шаг 4: Вызов из main -->\n                <div v-if=\"currentStep === 4\" class=\"step-panel\">\n                  <h2 class=\"step-title\">Вызов функции из main</h2>\n                  <div class=\"form-group\">\n                    <label for=\"input-format\">Формат входных данных</label>\n                    <textarea\n                        id=\"input-format\"\n                        v-model=\"taskData.inputFormat\"\n                        rows=\"3\"\n                        placeholder=\"Опишите формат входных данных\"\n                    ></textarea>\n                  </div>\n                  <div class=\"form-group\">\n                    <label for=\"output-format\">Формат выходных данных</label>\n                    <textarea\n                        id=\"output-format\"\n                        v-model=\"taskData.outputFormat\"\n                        rows=\"3\"\n                        placeholder=\"Опишите формат выходных данных\"\n                    ></textarea>\n                  </div>\n                  <div class=\"form-group\">\n                    <label>Пример вызова функции</label>\n                    <pre class=\"code-snippet\"><span>{{ generateMainCallExample() }}</span></pre>\n                  </div>\n                </div>\n\n                <!-- Шаг 5: Тесты -->\n                <div v-if=\"currentStep === 5\" class=\"step-panel\">\n                  <h2 class=\"step-title\">Тесты для задачи</h2>\n                  <div class=\"tests-container\">\n                    <div v-for=\"(test, index) in taskData.tests\" :key=\"index\" class=\"test-case\">\n                      <h4>Тест {{ index + 1 }}</h4>\n                      <div class=\"test-inputs\">\n                        <div class=\"form-group\">\n                          <label>Входные данные</label>\n                          <textarea\n                              v-model=\"test.input\"\n                              rows=\"3\"\n                              placeholder=\"Входные данные для теста\"\n                          ></textarea>\n                        </div>\n                        <div class=\"form-group\">\n                          <label>Ожидаемый результат</label>\n                          <textarea\n                              v-model=\"test.expectedOutput\"\n                              rows=\"3\"\n                              placeholder=\"Ожидаемый результат\"\n                          ></textarea>\n                        </div>\n                      </div>\n                      <div class=\"test-options\">\n                        <label class=\"checkbox-label\">\n                          <input\n                              type=\"checkbox\"\n                              v-model=\"test.isPublic\"\n                          >\n                          Показать студенту\n                        </label>\n                        <button @click=\"removeTest(index)\" class=\"btn-remove\">Удалить тест</button>\n                      </div>\n                    </div>\n                    <button @click=\"addTest\" class=\"btn-outline btn\">+ Добавить тест</button>\n                  </div>\n                </div>\n\n                <!-- Навигация по шагам -->\n                <div class=\"wizard-navigation\">\n                  <button\n                      v-if=\"currentStep > 1\"\n                      @click=\"previousStep\"\n                      class=\"btn-outline btn\"\n                  >\n                    Назад\n                  </button>\n                  <button\n                      v-if=\"currentStep < 5\"\n                      @click=\"nextStep\"\n                      class=\"btn-primary btn\"\n                  >\n                    Далее\n                  </button>\n                  <button\n                      v-if=\"currentStep === 5\"\n                      @click=\"saveTask\"\n                      class=\"btn-accent btn\"\n                  >\n                    Сохранить задачу\n                  </button>\n                </div>\n              </div>\n\n              <!-- Предпросмотр -->\n              <div class=\"preview-panel\">\n                <div class=\"panel-header\">\n                  <svg\n                      width=\"20\"\n                      xmlns=\"http://www.w3.org/2000/svg\"\n                      height=\"20\"\n                      viewBox=\"0 0 24 24\"\n                  >\n                    <path\n                        d=\"m16 18l6-6l-6-6M8 6l-6 6l6 6\"\n                        fill=\"none\"\n                        stroke=\"currentColor\"\n                        stroke-width=\"2\"\n                        stroke-linecap=\"round\"\n                        stroke-linejoin=\"round\"\n                    ></path>\n                  </svg>\n                  <span>Предпросмотр задачи</span>\n                </div>\n                <div class=\"preview-content\">\n                  <h3>{{ taskData.title || 'Название задачи' }}</h3>\n                  <p>{{ taskData.description || 'Описание задачи' }}</p>\n\n                  <h4>Функция для реализации:</h4>\n                  <pre class=\"code-block\"><span>{{ generateFunctionTemplate() }}</span></pre>\n\n                  <h4>Пример использования:</h4>\n                  <pre class=\"code-block\"><span>{{ generateMainCallExample() }}</span></pre>\n\n                  <div v-if=\"taskData.tests.some(t => t.isPublic)\" class=\"public-tests\">\n                    <h4>Примеры тестов:</h4>\n                    <div v-for=\"(test, index) in taskData.tests.filter(t => t.isPublic)\" :key=\"index\" class=\"test-example\">\n                      <p><strong>Вход:</strong> {{ test.input }}</p>\n                      <p><strong>Выход:</strong> {{ test.expectedOutput }}</p>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <app-footer></app-footer>\n  </div>\n</template>\n\n<script>\nimport DangerousHTML from 'dangerous-html/vue'\nimport AppNavigation from '../components/navigation'\nimport AppFooter from '../components/footer'\n\nexport default {\n  name: 'TaskTemplateBuilder',\n  props: {},\n  components: {\n    AppNavigation,\n    DangerousHTML,\n    AppFooter,\n  },\n  data() {\n    return {\n      currentStep: 1,\n      taskData: {\n        title: '',\n        description: '',\n        difficulty: 'medium',\n        functionName: '',\n        parameters: [{ name: '', type: 'int' }],\n        returnType: 'int',\n        functionDescription: '',\n        language: 'python',\n        libraries: [],\n        inputFormat: '',\n        outputFormat: '',\n        tests: [\n          { input: '', expectedOutput: '', isPublic: true }\n        ]\n      },\n      availableLibraries: [\n        { id: 'numpy', name: 'NumPy' },\n        { id: 'pandas', name: 'Pandas' },\n        { id: 'matplotlib', name: 'Matplotlib' },\n        { id: 'requests', name: 'Requests' }\n      ]\n    }\n  },\n  methods: {\n    nextStep() {\n      if (this.currentStep < 5) {\n        this.currentStep++\n      }\n    },\n    previousStep() {\n      if (this.currentStep > 1) {\n        this.currentStep--\n      }\n    },\n    addParameter() {\n      this.taskData.parameters.push({ name: '', type: 'int' })\n    },\n    removeParameter(index) {\n      if (this.taskData.parameters.length > 1) {\n        this.taskData.parameters.splice(index, 1)\n      }\n    },\n    toggleLibrary(libId) {\n      const index = this.taskData.libraries.indexOf(libId)\n      if (index > -1) {\n        this.taskData.libraries.splice(index, 1)\n      } else {\n        this.taskData.libraries.push(libId)\n      }\n    },\n    addTest() {\n      this.taskData.tests.push({ input: '', expectedOutput: '', isPublic: true })\n    },\n    removeTest(index) {\n      if (this.taskData.tests.length > 1) {\n        this.taskData.tests.splice(index, 1)\n      }\n    },\n    generateFunctionTemplate() {\n      if (!this.taskData.functionName) return '// Введите имя функции'\n\n      const params = this.taskData.parameters\n          .filter(p => p.name)\n          .map(p => `${p.name}: ${p.type}`)\n          .join(', ')\n\n      let template = `def ${this.taskData.functionName}(${params})`\n\n      if (this.taskData.returnType !== 'void') {\n        template += ` -> ${this.