lass=\"auth-container\">\n        <div class=\"auth-panel\">\n          <div class=\"auth-grid\">\n            <!-- Левая панель - Приветствие -->\n            <div class=\"auth-welcome\">\n              <div class=\"welcome-head\">\n                <h1 class=\"auth-title\">\n                  <svg\n                    width=\"32\"\n                    xmlns=\"http://www.w3.org/2000/svg\"\n                    height=\"32\"\n                    viewBox=\"0 0 24 24\"\n                  >\n                    <g\n                      fill=\"none\"\n                      stroke=\"currentColor\"\n                      stroke-width=\"2\"\n                      stroke-linecap=\"round\"\n                      stroke-linejoin=\"round\"\n                    >\n                      <path d=\"M19 21v-2a4 4 0 0 0-4-4H9a4 4 0 0 0-4 4v2\"></path>\n                      <circle r=\"4\" cx=\"12\" cy=\"7\"></circle>\n                    </g>\n                  </svg>\n                  Добро пожаловать\n                </h1>\n                <p class=\"auth-lead\">Войдите в свою мастерскую шаблонов</p>\n              </div>\n              \n              <div class=\"welcome-features\">\n                <div class=\"feature-item\">\n                  <svg\n                    width=\"20\"\n                    xmlns=\"http://www.w3.org/2000/svg\"\n                    height=\"20\"\n                    viewBox=\"0 0 24 24\"\n                  >\n                    <path\n                      d=\"m16 18l6-6l-6-6M8 6l-6 6l6 6\"\n                      fill=\"none\"\n                      stroke=\"currentColor\"\n                      stroke-width=\"2\"\n                      stroke-linecap=\"round\"\n                      stroke-linejoin=\"round\"\n                    ></path>\n                  </svg>\n                  <span>Создавайте учебные шаблоны</span>\n                </div>\n                <div class=\"feature-item\">\n                  <svg\n                    width=\"20\"\n                    xmlns=\"http://www.w3.org/2000/svg\"\n                    height=\"20\"\n                    viewBox=\"0 0 24 24\"\n                  >\n                    <path\n                      d=\"m16 6l4 14M12 6v14M8 8v12M4 4v16\"\n                      fill=\"none\"\n                      stroke=\"currentColor\"\n                      stroke-width=\"2\"\n                      stroke-linecap=\"round\"\n                      stroke-linejoin=\"round\"\n                    ></path>\n                  </svg>\n                  <span>Управляйте библиотеками</span>\n                </div>\n                <div class=\"feature-item\">\n                  <svg\n                    width=\"20\"\n                    xmlns=\"http://www.w3.org/2000/svg\"\n                    height=\"20\"\n                    viewBox=\"0 0 24 24\"\n                  >\n                    <g\n                      fill=\"none\"\n                      stroke=\"currentColor\"\n                      stroke-width=\"2\"\n                      stroke-linecap=\"round\"\n                      stroke-linejoin=\"round\"\n                    >\n                      <path d=\"M12 6v6l4 2\"></path>\n                      <circle r=\"10\" cx=\"12\" cy=\"12\"></circle>\n                    </g>\n                  </svg>\n                  <span>Отслеживайте прогресс студентов</span>\n                </div>\n              </div>\n              \n              <div class=\"welcome-stats\">\n                <div class=\"stat-badge\">\n                  <strong>1,920+</strong>\n                  <span>преподавателей</span>\n                </div>\n                <div class=\"stat-badge\">\n                  <strong>54,000+</strong>\n                  <span>студентов</span>\n                </div>\n              </div>\n            </div>\n\n            <!-- Правая панель - Формы -->\n            <div class=\"auth-forms\">\n              <!-- Форма входа -->\n              <div v-if=\"!isRegister\" class=\"auth-form active\">\n                <div class=\"form-header\">\n                  <h2 class=\"form-title\">Вход в систему</h2>\n                  <p class=\"form-subtitle\">Введите ваши учетные данные</p>\n                </div>\n                \n                <form @submit.prevent=\"handleLogin\" class=\"auth-form-content\">\n                  <div class=\"form-group\">\n                    <label for=\"login-email\">Email</label>\n                    <input\n                      id=\"login-email\"\n                      v-model=\"loginForm.email\"\n                      type=\"email\"\n                      required\n                      placeholder=\"your.email@example.com\"\n                      class=\"form-input\"\n                    >\n                  </div>\n                  \n                  <div class=\"form-group\">\n                    <label for=\"login-password\">Пароль</label>\n                    <div class=\"password-input-wrapper\">\n                      <input\n                        id=\"login-password\"\n                        v-model=\"loginForm.password\"\n                        :type=\"showLoginPassword ? 'text' : 'password'\"\n                        required\n                        placeholder=\"Введите ваш пароль\"\n                        class=\"form-input\"\n                      >\n                      <button\n                        type=\"button\"\n                        @click=\"showLoginPassword = !showLoginPassword\"\n                        class=\"password-toggle\"\n                        :aria-label=\"showLoginPassword ? 'Скрыть пароль' : 'Показать пароль'\"\n                      >\n                        <svg\n                          width=\"18\"\n                          xmlns=\"http://www.w3.org/2000/svg\"\n                          height=\"18\"\n                          viewBox=\"0 0 24 24\"\n                        >\n                          <g\n                            fill=\"none\"\n                            stroke=\"currentColor\"\n                            stroke-width=\"2\"\n                            stroke-linecap=\"round\"\n                            stroke-linejoin=\"round\"\n                          >\n                            <path d=\"M2 12s3-7 10-7 10 7 10 7-3 7-10 7-10-7-10-7Z\"></path>\n                            <circle r=\"3\" cx=\"12\" cy=\"12\"></circle>\n                          </g>\n                        </svg>\n                      </button>\n                    </div>\n                  </div>\n                  \n                  <div class=\"form-options\">\n                    <label class=\"checkbox-label\">\n                      <input\n                        type=\"checkbox\"\n                        v-model=\"loginForm.rememberMe\"\n                        class=\"checkbox-input\"\n                      >\n                      <span class=\"checkbox-custom\"></span>\n                      Запомнить меня\n                    </label>\n                    <a href=\"#forgot-password\" class=\"forgot-link\">Забыли пароль?</a>\n                  </div>\n                  \n                  <button\n                    type=\"submit\"\n                    :disabled=\"loading\"\n                    class=\"auth-submit btn-primary btn\"\n                  >\n                    <span v-if=\"loading\">Вход...</span>\n                    <span v-else>Войти в систему</span>\n                  </button>\n                  \n                  <div class=\"auth-switch\">\n                    <span>Нет аккаунта?</span>\n                    <button\n                      type=\"button\"\n                      @click=\"isRegister = true\"\n                      class=\"switch-link\"\n                    >\n                      Зарегистрироваться\n                    </button>\n                  </div>\n                </form>\n              </div>\n\n              <!-- Форма регистрации -->\n              <div v-else class=\"auth-form active\">\n                <div class=\"form-header\">\n                  <h2 class=\"form-title\">Регистрация</h2>\n                  <p class=\"form-subtitle\">Создайте новую учетную запись</p>\n                </div>\n                \n                <form @submit.prevent=\"handleRegister\" class=\"auth-form-content\">\n                  <div class=\"form-row\">\n                    <div class=\"form-group\">\n                      <label for=\"register-firstName\">Имя</label>\n                      <input\n                        id=\"register-firstName\"\n                        v-model=\"registerForm.firstName\"\n                        type=\"text\"\n                        required\n                        placeholder=\"Иван\"\n                        class=\"form-input\"\n                      >\n                    </div>\n                    <div class=\"form-group\">\n                      <label for=\"register-lastName\">Фамилия</label>\n                      <input\n                        id=\"register-lastName\"\n                        v-model=\"registerForm.lastName\"\n                        type=\"text\"\n                        required\n                        placeholder=\"Иванов\"\n                        class=\"form-input\"\n                      >\n                    </div>\n                  </div>\n                  \n                  <div class=\"form-group\">\n                    <label for=\"register-email\">Email</label>\n                    <input\n                      id=\"register-email\"\n                      v-model=\"registerForm.email\"\n                      type=\"email\"\n                      required\n                      placeholder=\"your.email@example.com\"\n                      class=\"form-input\"\n                    >\n                  </div>\n                  \n                  <div class=\"form-group\">\n                    <label for=\"register-password\">Пароль</label>\n                    <div class=\"password-input-wrapper\">\n                      <input\n                        id=\"register-password\"\n                        v-model=\"registerForm.password\"\n                        :type=\"showRegisterPassword ? 'text' : 'password'\"\n                        required\n                        placeholder=\"Создайте надежный пароль\"\n                        class=\"form-input\"\n                      >\n                      <button\n                        type=\"button\"\n                        @click=\"showRegisterPassword = !showRegisterPassword\"\n                        class=\"password-toggle\"\n                        :aria-label=\"showRegisterPassword ? 'Скрыть пароль' : 'Показать пароль'\"\n                      >\n                        <svg\n                          width=\"18\"\n                          xmlns=\"http://www.w3.org/2000/svg\"\n                          height=\"18\"\n                          viewBox=\"0 0 24 24\"\n                        >\n                          <g\n                            fill=\"none\"\n                            stroke=\"currentColor\"\n                            stroke-width=\"2\"\n                            stroke-linecap=\"round\"\n                            stroke-linejoin=\"round\"\n                          >\n                            <path d=\"M2 12s3-7 10-7 10 7 10 7-3 7-10 7-10-7-10-7Z\"></path>\n                            <circle r=\"3\" cx=\"12\" cy=\"12\"></circle>\n                          </g>\n                        </svg>\n                      </button>\n                    </div>\n                    <div class=\"password-strength\">\n                      <div\n                        class=\"strength-bar\"\n                        :class=\"passwordStrengthClass\"\n                      ></div>\n                      <span class=\"strength-text\">{{ passwordStrengthText }}</span>\n                    </div>\n                  </div>\n                  \n                  <div class=\"form-group\">\n                    <label for=\"register-confirmPassword\">Подтверждение пароля</label>\n                    <input\n                      id=\"register-confirmPassword\"\n                      v-model=\"registerForm.confirmPassword\"\n                      type=\"password\"\n                      required\n                      placeholder=\"Повторите ваш пароль\"\n                      class=\"form-input\"\n                      :class=\"{ error: registerForm.password !== registerForm.confirmPassword && registerForm.confirmPassword }\"\n                    >\n                  </div>\n                  \n                  <div class=\"form-options\">\n                    <label class=\"checkbox-label\">\n                      <input\n                        type=\"checkbox\"\n                        v-model=\"registerForm.acceptTerms\"\n                        required\n                        class=\"checkbox-input\"\n                      >\n                      <span class=\"checkbox-custom\"></span>\n                      Я принимаю \n                      <a href=\"#terms\" class=\"terms-link\">условия использования</a>\n                    </label>\n                  </div>\n                  \n                  <button\n                    type=\"submit\"\n                    :disabled=\"loading || !passwordsMatch\"\n                    class=\"auth-submit btn-primary btn\"\n                  >\n                    <span v-if=\"loading\">Регистрация...</span>\n                    <span v-else>Создать аккаунт</span>\n                  </button>\n                  \n                  <div class=\"auth-switch\">\n                    <span>Уже есть аккаунт?</span>\n                    <button\n                      type=\"button\"\n                      @click=\"isRegister = false\"\n                      class=\"switch-link\"\n                    >\n                      Войти\n                    </button>\n                  </div>\n                </form>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <app-footer></app-footer>\n    \n    <div class=\"auth-container27\">\n      <div class=\"auth-container28\">\n        <DangerousHTML\n          html=\"<style>\n          @keyframes authSlideIn {\n            from { opacity: 0; transform: translateX(20px); }\n            to { opacity: 1; transform: translateX(0); }\n          }\n          </style>\"\n        ></DangerousHTML>\n      </div>\n    </div>\n    \n    <div class=\"auth-container29\">\n      <div class=\"auth-container30\">\n        <DangerousHTML\n          html=\"<script defer data-name='auth'>\n  (function(){\n    // Password strength calculation\n    function calculatePasswordStrength(password) {\n      let strength = 0\n      if (password.length >= 8) strength++\n      if (/[A-Z]/.test(password)) strength++\n      if (/[a-z]/.test(password)) strength++\n      if (/[0-9]/.test(password)) strength++\n      if (/[^A-Za-z0-9]/.test(password)) strength++\n      return strength\n    }\n\n    const passwordInput = document.getElementById('register-password')\n    if (passwordInput) {\n      passwordInput.addEventListener('input', function() {\n        const strength = calculatePasswordStrength(this.value)\n        const strengthBar = document.querySelector('.strength-bar')\n        const strengthText = document.querySelector('.strength-text')\n        \n        const classes = ['very-weak', 'weak', 'medium', 'strong', 'very-strong']\n        const texts = ['Очень слабый', 'Слабый', 'Средний', 'Надежный', 'Очень надежный']\n        \n        strengthBar.className = 'strength-bar ' + (classes[strength] || 'very-weak')\n        strengthText.textContent = texts[strength] || 'Очень слабый'\n      })\n    }\n\n    // Form switching animation\n    const authForms = document.querySelectorAll('.auth-form')\n    authForms.forEach(form => {\n      form.style.opacity = '0'\n      form.style.transform = 'translateX(20px)'\n      form.style.transition = 'all 0.3s ease-in-out'\n    })\n\n    // Observe active form\n    const observer = new MutationObserver(function(mutations) {\n      mutations.forEach(function(mutation) {\n        if (mutation.type === 'attributes' && mutation.attributeName === 'class') {\n          const form = mutation.target\n          if (form.classList.contains('active')) {\n            form.style.opacity = '1'\n            form.style.transform = 'translateX(0)'\n          }\n        }\n      })\n    })\n\n    authForms.forEach(form => {\n      observer.observe(form, { attributes: true })\n    })\n\n    // Add vintage paper effect to form inputs\n    const formInputs = document.querySelectorAll('.form-input')\n    formInputs.forEach(input => {\n      input.addEventListener('focus', function() {\n        this.parentElement.classList.add('focused')\n      })\n      \n      input.addEventListener('blur', function() {\n        this.parentElement.classList.remove('focused')\n      })\n    })\n  })()\n  </script>\"\n        ></DangerousHTML>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport DangerousHTML from 'dangerous-html/vue'\nimport AppNavigation from '../components/navigation'\nimport AppFooter from '../components/footer'\n\nexport default {\n  name: 'Auth',\n  components: {\n    AppNavigation,\n    AppFooter,\n    DangerousHTML,\n  },\n  data() {\n    return {\n      isRegister: false,\n      loading: false,\n      showLoginPassword: false,\n      showRegisterPassword: false,\n      loginForm: {\n        email: '',\n        password: '',\n        rememberMe: false,\n      },\n      registerForm: {\n        firstName: '',\n        lastName: '',\n        email: '',\n        password: '',\n        confirmPassword: '',\n        acceptTerms: false,\n      },\n    }\n  },\n  computed: {\n    passwordsMatch() {\n      return this.registerForm.password === this.registerForm.confirmPassword\n    },\n    passwordStrength() {\n      const password = this.registerForm.password\n      if (!password) return 0\n      \n      let strength = 0\n      if (password.length >= 8) strength++\n      if (/[A-Z]/.test(password)) strength++\n      if (/[a-z]/.test(password)) strength++\n      if (/[0-9]/.test(password)) strength++\n      if (/[^A-Za-z0-9]/.test(password)) strength++\n      \n      return strength\n    },\n    passwordStrengthClass() {\n      const classes = ['very-weak', 'weak', 'medium', 'strong', 'very-strong']\n      return classes[this.passwordStrength] || 'very-weak'\n    },\n    passwordStrengthText() {\n      const texts = ['Очень слабый', 'Слабый', 'Средний', 'Надежный', 'Очень надежный']\n      return texts[this.passwordStrength] || 'Очень слабый'\n    },\n  },\n  methods: {\n    async handleLogin() {\n      this.loading = true\n      \n      try {\n        // Имитация API запроса\n        await new Promise(resolve => setTimeout(resolve, 1500))\n        \n        // Сохраняем данные пользователя\n        const userData = {\n          id: 1,\n          email: this.loginForm.email,\n          firstName: 'Иван',\n          lastName: 'Иванов',\n          role: 'teacher',\n          avatar: null,\n        }\n        \n        localStorage.setItem('user', JSON.stringify(userData))\n        localStorage.setItem('token', 'mock-jwt-token')\n        \n        if (this.loginForm.rememberMe) {\n          localStorage.setItem('rememberMe', 'true')\n        }\n        \n        // Перенаправляем на главную страницу\n        this.$router.push('/')\n        \n      } catch (error) {\n        console.error('Login error:', error)\n        // Здесь можно добавить уведомление об ошибке\n      } finally {\n        this.loading = false\n      }\n    },\n    \n    async handleRegister() {\n      if (!this.passwordsMatch) {\n        return\n      }\n      \n      this.loading = true\n      \n      try {\n        // Имитация API запроса\n        await new