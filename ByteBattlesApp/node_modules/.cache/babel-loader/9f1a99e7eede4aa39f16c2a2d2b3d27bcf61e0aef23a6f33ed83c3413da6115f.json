[1, 3, 4, 5]]);\n      }))();\n    },\n    handleRegister: function handleRegister() {\n      var _this2 = this;\n      return _asyncToGenerator(/*#__PURE__*/_regenerator().m(function _callee2() {\n        var userData, _t2;\n        return _regenerator().w(function (_context2) {\n          while (1) switch (_context2.p = _context2.n) {\n            case 0:\n              if (_this2.passwordsMatch) {\n                _context2.n = 1;\n                break;\n              }\n              return _context2.a(2);\n            case 1:\n              _this2.loading = true;\n              _context2.p = 2;\n              _context2.n = 3;\n              return new Promise(function (resolve) {\n                return setTimeout(resolve, 2000);\n              });\n            case 3:\n              // Автоматически входим после регистрации\n              userData = {\n                id: Date.now(),\n                email: _this2.registerForm.email,\n                firstName: _this2.registerForm.firstName,\n                lastName: _this2.registerForm.lastName,\n                role: 'teacher',\n                avatar: null\n              };\n              localStorage.setItem('user', JSON.stringify(userData));\n              localStorage.setItem('token', 'mock-jwt-token');\n\n              // Перенаправляем на главную страницу\n              _this2.$router.push('/');\n              _context2.n = 5;\n              break;\n            case 4:\n              _context2.p = 4;\n              _t2 = _context2.v;\n              console.error('Registration error:', _t2);\n              // Здесь можно добавить уведомление об ошибке\n            case 5:\n              _context2.p = 5;\n              _this2.loading = false;\n              return _context2.f(5);\n            case 6:\n              return _context2.a(2);\n          }\n        }, _callee2, null, [[2, 4, 5, 6]]);\n      }))();\n    }\n  }\n};","map":{"version":3,"names":["DangerousHTML","AppNavigation","AppFooter","name"