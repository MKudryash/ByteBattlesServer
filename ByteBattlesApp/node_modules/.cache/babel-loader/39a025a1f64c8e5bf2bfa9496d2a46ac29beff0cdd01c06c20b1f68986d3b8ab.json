{"ast":null,"code":"import \"core-js/modules/es.symbol.js\";\nimport \"core-js/modules/es.symbol.description.js\";\nimport \"core-js/modules/es.array.filter.js\";\nimport \"core-js/modules/es.array.find.js\";\nimport \"core-js/modules/es.array.includes.js\";\nimport \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.function.name.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/es.string.includes.js\";\nimport DangerousHTML from 'dangerous-html/vue';\nimport AppNavigation from '../components/navigation';\nimport AppFooter from '../components/footer';\nexport default {\n  name: 'TaskList',\n  components: {\n    AppNavigation: AppNavigation,\n    DangerousHTML: DangerousHTML,\n    AppFooter: AppFooter\n  },\n  data: function data() {\n    return {\n      viewMode: 'grid',\n      difficultyFilter: '',\n      languageFilter: '',\n      categoryFilter: '',\n      searchTerm: '',\n      tasks: [{\n        id: 1,\n        title: '–°—É–º–º–∞ —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –º–∞—Å—Å–∏–≤–∞',\n        description: '–ù–∞–ø–∏—à–∏—Ç–µ —Ñ—É–Ω–∫—Ü–∏—é –¥–ª—è –≤—ã—á–∏—Å–ª–µ–Ω–∏—è —Å—É–º–º—ã –≤—Å–µ—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –º–∞—Å—Å–∏–≤–∞',\n        difficulty: 'easy',\n        language: 'Python',\n        category: 'algorithms',\n        tags: ['–º–∞—Å—Å–∏–≤—ã', '—Å—É–º–º–∞', '–±–∞–∑–æ–≤—ã–µ'],\n        completedCount: 156,\n        rating: 4.2,\n        avgTime: 15,\n        isAuthor: true\n      }\n      // ... –±–æ–ª—å—à–µ –∑–∞–¥–∞—á\n      ]\n    };\n  },\n  computed: {\n    filteredTasks: function filteredTasks() {\n      var _this = this;\n      return this.tasks.filter(function (task) {\n        var matchesDifficulty = !_this.difficultyFilter || task.difficulty === _this.difficultyFilter;\n        var matchesLanguage = !_this.languageFilter || task.language === _this.languageFilter;\n        var matchesCategory = !_this.categoryFilter || task.category === _this.categoryFilter;\n        var matchesSearch = !_this.searchTerm || task.title.toLowerCase().includes(_this.searchTerm.toLowerCase()) || task.description.toLowerCase().includes(_this.searchTerm.toLowerCase());\n        return matchesDifficulty && matchesLanguage && matchesCategory && matchesSearch;\n      });\n    },\n    hasActiveFilters: function hasActiveFilters() {\n      return this.difficultyFilter || this.languageFilter || this.categoryFilter || this.searchTerm;\n    }\n  },\n  methods: {\n    resetFilters: function resetFilters() {\n      this.difficultyFilter = '';\n      this.languageFilter = '';\n      this.categoryFilter = '';\n      this.searchTerm = '';\n    },\n    editTask: function editTask(taskId) {\n      this.$router.push(\"/tasks/\".concat(taskId, \"/edit\"));\n    },\n    getDifficultyLabel: function getDifficultyLabel(difficulty) {\n      var labels = {\n        easy: '–õ–µ–≥–∫–∞—è',\n        medium: '–°—Ä–µ–¥–Ω—è—è',\n        hard: '–°–ª–æ–∂–Ω–∞—è'\n      };\n      return labels[difficulty] || difficulty;\n    },\n    getLanguageName: function getLanguageName(langId) {\n      var lang = this.availableLanguages.find(function (l) {\n        return l.id === langId;\n      });\n      return lang ? lang.name : langId;\n    },\n    getCategoryLabel: function getCategoryLabel(category) {\n      var labels = {\n        algorithms: '–ê–ª–≥–æ—Ä–∏—Ç–º—ã',\n        'data-structures': '–°—Ç—Ä—É–∫—Ç—É—Ä—ã –¥–∞–Ω–Ω—ã—Ö',\n        oop: '–û–û–ü',\n        web: '–í–µ–±-—Ä–∞–∑—Ä–∞–±–æ—Ç–∫–∞'\n      };\n      return labels[category] || category;\n    },\n    getLanguageIcon: function getLanguageIcon(language) {\n      var icons = {\n        Python: 'üêç',\n        Java: '‚òï',\n        JavaScript: 'üìú',\n        'C++': '‚ö°'\n      };\n      return icons[language] || 'üíª';\n    }\n  }\n};","map":{"version":3,"names":["DangerousHTML","AppNavigation","AppFooter","name","components","data","viewMode","difficultyFilter","languageFilter","categoryFilter","searchTerm","tasks","id","title","description","difficulty","language","category","tags","completedCount","rating","avgTime","isAuthor","computed","filteredTasks","_this","filter","task","matchesDifficulty","matchesLanguage","matchesCategory","matchesSearch","toLowerCase","includes","hasActiveFilters","methods","resetFilters","editTask","taskId","$router","push","concat","getDifficultyLabel","labels","easy","medium","hard","getLanguageName","langId","lang","availableLanguages","find","l","getCategoryLabel","algorithms","oop","web","getLanguageIcon","icons","Python","Java","JavaScript"],"sources":["src/views/TaskList.vue"],"sourcesContent":["<template>\n  <div class=\"task-list-container\">\n    <app-navigation></app-navigation>\n\n    <div class=\"task-list-wrapper\">\n      <DangerousHTML\n          html=\"<style>\n          .task-list-container {\n            min-height: 100vh;\n            background: var(--color-surface);\n            padding: var(--spacing-2xl) 0;\n          }\n          /* ... –æ—Å—Ç–∞–ª—å–Ω—ã–µ —Å—Ç–∏–ª–∏ –∫–∞–∫ –≤ –¥—Ä—É–≥–∏—Ö –∫–æ–º–ø–æ–Ω–µ–Ω—Ç–∞—Ö ... */\n        </style>\"\n      ></DangerousHTML>\n\n      <section class=\"task-list-section\" role=\"main\" aria-label=\"–°–ø–∏—Å–æ–∫ –∑–∞–¥–∞—á\">\n        <div class=\"container\">\n          <!-- –ó–∞–≥–æ–ª–æ–≤–æ–∫ –∏ —Ñ–∏–ª—å—Ç—Ä—ã -->\n          <div class=\"page-header\">\n            <div class=\"title-section\">\n              <h1 class=\"page-title\">\n                <span class=\"title-icon\">üìö</span>\n                –ë–∏–±–ª–∏–æ—Ç–µ–∫–∞ –∑–∞–¥–∞—á\n              </h1>\n              <p class=\"page-subtitle\">\n                –ò–∑—É—á–∞–π—Ç–µ –ø—Ä–æ–≥—Ä–∞–º–º–∏—Ä–æ–≤–∞–Ω–∏–µ —á–µ—Ä–µ–∑ —Ä–µ—à–µ–Ω–∏–µ –ø—Ä–∞–∫—Ç–∏—á–µ—Å–∫–∏—Ö –∑–∞–¥–∞—á\n              </p>\n            </div>\n\n            <!-- –ë—ã—Å—Ç—Ä—ã–µ –¥–µ–π—Å—Ç–≤–∏—è -->\n            <div class=\"quick-actions retro-card\">\n              <div class=\"actions-grid\">\n                <router-link to=\"/tasks/create\" class=\"btn-primary\">\n                  <span class=\"btn-icon\">‚ûï</span>\n                  –°–æ–∑–¥–∞—Ç—å –∑–∞–¥–∞—á—É\n                </router-link>\n                <div class=\"view-controls\">\n                  <button\n                      @click=\"viewMode = 'grid'\"\n                      :class=\"['view-btn', { 'active': viewMode === 'grid' }]\"\n                  >\n                    <span class=\"btn-icon\">‚èπÔ∏è</span>\n                  </button>\n                  <button\n                      @click=\"viewMode = 'list'\"\n                      :class=\"['view-btn', { 'active': viewMode === 'list' }]\"\n                  >\n                    <span class=\"btn-icon\">üìã</span>\n                  </button>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <!-- –§–∏–ª—å—Ç—Ä—ã -->\n          <div class=\"tasks-filters retro-card\">\n            <div class=\"filters-header\">\n              <h3 class=\"filters-title\">\n                <span class=\"filters-icon\">üîç</span>\n                –§–∏–ª—å—Ç—Ä—ã –∑–∞–¥–∞—á\n              </h3>\n              <button\n                  @click=\"resetFilters\"\n                  class=\"btn-text btn-sm reset-filters-btn\"\n                  :disabled=\"!hasActiveFilters\"\n              >\n                <span class=\"btn-icon\">üîÑ</span>\n                –°–±—Ä–æ—Å–∏—Ç—å\n              </button>\n            </div>\n\n            <div class=\"filters-grid\">\n              <div class=\"filter-group\">\n                <label class=\"filter-label\">–°–ª–æ–∂–Ω–æ—Å—Ç—å:</label>\n                <div class=\"select-wrapper vintage-border\">\n                  <select v-model=\"difficultyFilter\" class=\"filter-select\">\n                    <option value=\"\">–í—Å–µ —É—Ä–æ–≤–Ω–∏</option>\n                    <option value=\"easy\">–õ–µ–≥–∫–∏–µ</option>\n                    <option value=\"medium\">–°—Ä–µ–¥–Ω–∏–µ</option>\n                    <option value=\"hard\">–°–ª–æ–∂–Ω—ã–µ</option>\n                  </select>\n                  <span class=\"select-arrow\">‚ñº</span>\n                </div>\n              </div>\n\n              <div class=\"filter-group\">\n                <label class=\"filter-label\">–Ø–∑—ã–∫:</label>\n                <div class=\"select-wrapper vintage-border\">\n                  <select v-model=\"languageFilter\" class=\"filter-select\">\n                    <option value=\"\">–í—Å–µ —è–∑—ã–∫–∏</option>\n                    <option v-for=\"lang in availableLanguages\" :key=\"lang.id\" :value=\"lang.id\">\n                      {{ lang.name }}\n                    </option>\n                  </select>\n                  <span class=\"select-arrow\">‚ñº</span>\n                </div>\n              </div>\n\n              <div class=\"filter-group\">\n                <label class=\"filter-label\">–ö–∞—Ç–µ–≥–æ—Ä–∏—è:</label>\n                <div class=\"select-wrapper vintage-border\">\n                  <select v-model=\"categoryFilter\" class=\"filter-select\">\n                    <option value=\"\">–í—Å–µ –∫–∞—Ç–µ–≥–æ—Ä–∏–∏</option>\n                    <option value=\"algorithms\">–ê–ª–≥–æ—Ä–∏—Ç–º—ã</option>\n                    <option value=\"data-structures\">–°—Ç—Ä—É–∫—Ç—É—Ä—ã –¥–∞–Ω–Ω—ã—Ö</option>\n                    <option value=\"oop\">–û–û–ü</option>\n                    <option value=\"web\">–í–µ–±-—Ä–∞–∑—Ä–∞–±–æ—Ç–∫–∞</option>\n                  </select>\n                  <span class=\"select-arrow\">‚ñº</span>\n                </div>\n              </div>\n\n              <div class=\"search-group\">\n                <label class=\"filter-label\">–ü–æ–∏—Å–∫:</label>\n                <div class=\"search-input-wrapper vintage-border\">\n                  <span class=\"search-icon\">üîç</span>\n                  <input\n                      type=\"text\"\n                      v-model=\"searchTerm\"\n                      placeholder=\"–ù–∞–∑–≤–∞–Ω–∏–µ –∑–∞–¥–∞—á–∏...\"\n                      class=\"search-input\"\n                  >\n                  <button\n                      v-if=\"searchTerm\"\n                      @click=\"searchTerm = ''\"\n                      class=\"clear-search-btn\"\n                      type=\"button\"\n                  >\n                    <span class=\"clear-icon\">√ó</span>\n                  </button>\n                </div>\n              </div>\n            </div>\n\n            <!-- –ê–∫—Ç–∏–≤–Ω—ã–µ —Ñ–∏–ª—å—Ç—Ä—ã -->\n            <div class=\"active-filters\" v-if=\"hasActiveFilters\">\n              <div class=\"active-filters-label\">–ê–∫—Ç–∏–≤–Ω—ã–µ —Ñ–∏–ª—å—Ç—Ä—ã:</div>\n              <div class=\"active-filters-tags\">\n                <span\n                    v-if=\"difficultyFilter\"\n                    class=\"filter-tag\"\n                    @click=\"difficultyFilter = ''\"\n                >\n                  –°–ª–æ–∂–Ω–æ—Å—Ç—å: {{ getDifficultyLabel(difficultyFilter) }}\n                  <span class=\"tag-remove\">√ó</span>\n                </span>\n                <span\n                    v-if=\"languageFilter\"\n                    class=\"filter-tag\"\n                    @click=\"languageFilter = ''\"\n                >\n                  –Ø–∑—ã–∫: {{ getLanguageName(languageFilter) }}\n                  <span class=\"tag-remove\">√ó</span>\n                </span>\n                <span\n                    v-if=\"categoryFilter\"\n                    class=\"filter-tag\"\n                    @click=\"categoryFilter = ''\"\n                >\n                  –ö–∞—Ç–µ–≥–æ—Ä–∏—è: {{ getCategoryLabel(categoryFilter) }}\n                  <span class=\"tag-remove\">√ó</span>\n                </span>\n                <span\n                    v-if=\"searchTerm\"\n                    class=\"filter-tag\"\n                    @click=\"searchTerm = ''\"\n                >\n                  –ü–æ–∏—Å–∫: \"{{ searchTerm }}\"\n                  <span class=\"tag-remove\">√ó</span>\n                </span>\n              </div>\n            </div>\n          </div>\n\n          <!-- –°–ø–∏—Å–æ–∫ –∑–∞–¥–∞—á -->\n          <div class=\"tasks-container\">\n            <div :class=\"['tasks-grid', viewMode]\">\n              <div\n                  v-for=\"task in filteredTasks\"\n                  :key=\"task.id\"\n                  class=\"task-card retro-card\"\n              >\n                <div class=\"task-header\">\n                  <div class=\"task-meta\">\n                    <span class=\"task-difficulty\" :class=\"task.difficulty\">\n                      {{ getDifficultyLabel(task.difficulty) }}\n                    </span>\n                    <span class=\"task-language\">\n                      <span class=\"lang-icon\">{{ getLanguageIcon(task.language) }}</span>\n                      {{ task.language }}\n                    </span>\n                  </div>\n                  <div class=\"task-actions\">\n                    <button\n                        v-if=\"task.isAuthor\"\n                        @click=\"editTask(task.id)\"\n                        class=\"btn-text btn-sm\"\n                        title=\"–†–µ–¥–∞–∫—Ç–∏—Ä–æ–≤–∞—Ç—å\"\n                    >\n                      <span class=\"btn-icon\">‚úèÔ∏è</span>\n                    </button>\n                  </div>\n                </div>\n\n                <h3 class=\"task-title\">{{ task.title }}</h3>\n                <p class=\"task-description\">{{ task.description }}</p>\n\n                <div class=\"task-stats\">\n                  <div class=\"stat\">\n                    <span class=\"stat-icon\">‚úÖ</span>\n                    <span class=\"stat-value\">{{ task.completedCount }}</span>\n                  </div>\n                  <div class=\"stat\">\n                    <span class=\"stat-icon\">‚≠ê</span>\n                    <span class=\"stat-value\">{{ task.rating }}/5</span>\n                  </div>\n                  <div class=\"stat\">\n                    <span class=\"stat-icon\">‚è±Ô∏è</span>\n                    <span class=\"stat-value\">{{ task.avgTime }}–º</span>\n                  </div>\n                </div>\n\n                <div class=\"task-footer\">\n                  <div class=\"task-tags\">\n                    <span\n                        v-for=\"tag in task.tags.slice(0, 3)\"\n                        :key=\"tag\"\n                        class=\"tag\"\n                    >\n                      {{ tag }}\n                    </span>\n                    <span\n                        v-if=\"task.tags.length > 3\"\n                        class=\"tag-more\"\n                    >\n                      +{{ task.tags.length - 3 }}\n                    </span>\n                  </div>\n                  <router-link\n                      :to=\"`/tasks/${task.id}`\"\n                      class=\"btn-outline btn-sm\"\n                  >\n                    <span class=\"btn-icon\">üëÅÔ∏è</span>\n                    –†–µ—à–∞—Ç—å\n                  </router-link>\n                </div>\n              </div>\n            </div>\n\n            <!-- –ü—É—Å—Ç–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ -->\n            <div class=\"empty-state retro-card\" v-if=\"filteredTasks.length === 0\">\n              <div class=\"empty-icon\">üîç</div>\n              <h3>–ó–∞–¥–∞—á–∏ –Ω–µ –Ω–∞–π–¥–µ–Ω—ã</h3>\n              <p>–ü–æ–ø—Ä–æ–±—É–π—Ç–µ –∏–∑–º–µ–Ω–∏—Ç—å –ø–∞—Ä–∞–º–µ—Ç—Ä—ã –ø–æ–∏—Å–∫–∞ –∏–ª–∏ —Å–æ–∑–¥–∞—Ç—å –Ω–æ–≤—É—é –∑–∞–¥–∞—á—É</p>\n              <router-link to=\"/tasks/create\" class=\"btn-primary\">\n                <span class=\"btn-icon\">‚ûï</span>\n                –°–æ–∑–¥–∞—Ç—å –∑–∞–¥–∞—á—É\n              </router-link>\n            </div>\n          </div>\n        </div>\n      </section>\n    </div>\n\n    <app-footer></app-footer>\n  </div>\n</template>\n\n<script>\nimport DangerousHTML from 'dangerous-html/vue'\nimport AppNavigation from '../components/navigation'\nimport AppFooter from '../components/footer'\n\nexport default {\n  name: 'TaskList',\n  components: {\n    AppNavigation,\n    DangerousHTML,\n    AppFooter,\n  },\n  data() {\n    return {\n      viewMode: 'grid',\n      difficultyFilter: '',\n      languageFilter: '',\n      categoryFilter: '',\n      searchTerm: '',\n      tasks: [\n        {\n          id: 1,\n          title: '–°—É–º–º–∞ —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –º–∞—Å—Å–∏–≤–∞',\n          description: '–ù–∞–ø–∏—à–∏—Ç–µ —Ñ—É–Ω–∫—Ü–∏—é –¥–ª—è –≤—ã—á–∏—Å–ª–µ–Ω–∏—è —Å—É–º–º—ã –≤—Å–µ—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –º–∞—Å—Å–∏–≤–∞',\n          difficulty: 'easy',\n          language: 'Python',\n          category: 'algorithms',\n          tags: ['–º–∞—Å—Å–∏–≤—ã', '—Å—É–º–º–∞', '–±–∞–∑–æ–≤—ã–µ'],\n          completedCount: 156,\n          rating: 4.2,\n          avgTime: 15,\n          isAuthor: true\n        },\n        // ... –±–æ–ª—å—à–µ –∑–∞–¥–∞—á\n      ]\n    }\n  },\n  computed: {\n    filteredTasks() {\n      return this.tasks.filter(task => {\n        const matchesDifficulty = !this.difficultyFilter || task.difficulty === this.difficultyFilter\n        const matchesLanguage = !this.languageFilter || task.language === this.languageFilter\n        const matchesCategory = !this.categoryFilter || task.category === this.categoryFilter\n        const matchesSearch = !this.searchTerm ||\n            task.title.toLowerCase().includes(this.searchTerm.toLowerCase()) ||\n            task.description.toLowerCase().includes(this.searchTerm.toLowerCase())\n\n        return matchesDifficulty && matchesLanguage && matchesCategory && matchesSearch\n      })\n    },\n    hasActiveFilters() {\n      return this.difficultyFilter || this.languageFilter || this.categoryFilter || this.searchTerm\n    }\n  },\n  methods: {\n    resetFilters() {\n      this.difficultyFilter = ''\n      this.languageFilter = ''\n      this.categoryFilter = ''\n      this.searchTerm = ''\n    },\n  